{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include": ["./lib/", "./README.md"],
        "exclude": ["./node_modules"],
        "includePattern": "/*.js",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "referenceTitle": "discord-dialogflow",
        "disableSort": false,
        "collapse": true
    },
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "private": false,
        "recurse": true,
        "template": "./node_modules/jsdoc-template"
    }
}
